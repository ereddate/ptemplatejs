<template p-template="my-subslist">
	<ul class="list show" style="{{mysubs_data && mysubs_data.length > 0 ? 'display:block' : 'display:none'}}">
		<li p-express:for="i in mysubs_data">
			<article class="item">
				<header>
					<h1><a href="#" p-router:href="/teach_info?usid={{teacherId}}&url={{studioUrl}}">
						<lazyload src="{{imageUrl}}" class="loaded">
							<img src="data:image/svg+xml;base64," alt="{{teacherName}}" />
						</lazyload>
					</a></h1>
					<div>
						<h2><a href="#" p-router:href="/teach_info?usid={{teacherId}}">{{teacherName}}</a><i class="v"></i></h2>
						<p><span>{{createTime | toTime}}</span><span>{{commentNum | empty : 0}}<i class="icon-comm comm"></i></span></p>
					</div>
				</header>
				<div class="item_main">
					<h3>{{articleTitle | limitTo : 15}}</h3>
					<p>{{articleIntroduce | limitTo : 70}}... <a href="{{articleUrl}}" target="_blank">查看全文</a></p>
				</div>
			</article>
		</li>
	</ul>
</template>